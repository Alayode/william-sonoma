  "description": "Welcome to the Williams-Sonoma Markup",
  "main": "server.js",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
